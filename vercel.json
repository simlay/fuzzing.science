{
  "build": {
    "env": {
      "HUGO_VERSION": "0.87.0"
    }
  },
  "rewrites": [
    { "source": "/papers", "destination": "https://papers.fuzzing.science" },
    { "source": "/papers/(.*)", "destination": "https://papers.fuzzing.science/$1" },
    { "source": "/vulnerable-kext", "destination": "https://vulnerable-kext.fuzzing.science" },
    { "source": "/vulnerable-kext/(.*)", "destination": "https://vulnerable-kext.fuzzing.science/$1" }
  ],
  "trailingSlash": true
}